application: your-application-name-from-dev-console
version: 1
runtime: python27
api_version: 1
threadsafe: yes


handlers:
- url: /favicon\.ico
  static_files: favicon.ico
  upload: favicon\.ico

- url: .*
  script: main.app
  login: admin
  secure: always


libraries:
- name: webapp2
  version: latest

- name: pycrypto
  version: latest
